{
    "name": "n8n-nodes-vtex-apis",
    "version": "0.3.2",
    "description": "n8n community node for VTEX e-commerce platform",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "vtex",
        "ecommerce",
        "e-commerce",
        "marketplace",
        "catalog",
        "inventory"
    ],
    "license": "MIT",
    "homepage": "https://github.com/leosepulveda/n8n-nodes-vtex",
    "author": {
        "name": "Leonardo SepÃºlveda",
        "email": "lsepulvedatabares@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leosepulveda/n8n-nodes-vtex.git"
    },
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && npm run copy:assets",
        "copy:assets": "copyfiles -u 1 'nodes/**/*.{png,svg}' dist/",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials --ext .ts",
        "lintfix": "eslint nodes credentials --ext .ts --fix",
        "prepublishOnly": "npm run build && npm run lint"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/VtexApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Vtex/Vtex.node.js"
        ]
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "copyfiles": "^2.4.1",
        "eslint": "^8.50.0",
        "eslint-plugin-n8n-nodes-base": "^1.16.1",
        "prettier": "^3.0.0",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "n8n-workflow": "^1.0.0"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}